    "name": "Morten Trydal",
    "url": "https://github.com/trydis"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/trydis/csversion.git"
  },
  "bugs": {
    "url": "https://github.com/trydis/csversion/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/trydis/csversion/blob/master/LICENSE-MIT"
    }
